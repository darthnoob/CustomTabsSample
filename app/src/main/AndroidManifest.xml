<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.sakebook.android.sample.customtabssample"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LauncherActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ui.PrefetchActivity">
        </activity>
        <activity android:name=".ui.SessionBottombarActivity">
        </activity>
        <activity android:name=".ui.BottombarActivity">
        </activity>

        <service
            android:name="org.chromium.customtabsclient.shared.KeepAliveService"
            android:exported="true"/>

        <receiver android:name=".receivers.BottombarBroadcastReceiver"/>
        <receiver android:name=".receivers.SessionBottombarBroadcastReceiver"/>

        <activity android:name=".ui.DeprecatedBottombarActivity">
        </activity>
        <activity android:name=".ui.CustomAnimationActivity">
        </activity>
        <activity android:name=".ui.CustomToolbarActivity">
        </activity>
    </application>

</manifest>